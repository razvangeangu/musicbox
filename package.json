{
  "name": "@razvangeangu/musicbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "checkTs": "tsc --noEmit",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext js,ts,tsx",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "lint": "yarn run eslint src",
    "lint:commit": "commitlint --edit",
    "lint:css": "stylelint src/*/*.{ts,tsx} src/*/*/*.{ts,tsx} src/*/*/*/*.{ts,tsx}",
    "lint:fix": "yarn run eslint --fix src",
    "prepare": "husky install",
    "prettify": "prettier --write",
    "release": "standard-version",
    "start": "react-scripts start",
    "start:prod": "yarn run build && serve -s build",
    "test": "react-scripts test",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_moduless/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "dayjs": "1.11.0",
    "i18next": "21.6.14",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-scanner": "3.1.0",
    "react": "17.0.2",
    "react-app-polyfill": "3.0.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.3",
    "react-i18next": "11.16.2",
    "react-is": "17.0.2",
    "react-redux": "7.2.8",
    "react-responsive-carousel": "3.2.23",
    "react-router-dom": "6.3.0",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "13.0.0",
    "styled-components": "5.3.5",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-syntax-flow": "7.16.7",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@babel/register": "7.17.7",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@reduxjs/toolkit": "1.8.1",
    "@stylelint/postcss-css-in-js": "0.37.2",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@types/express": "4.17.13",
    "@types/fontfaceobserver": "2.1.0",
    "@types/gapi": "0.0.41",
    "@types/gapi.auth2": "0.0.56",
    "@types/gapi.client.youtube": "3.0.2",
    "@types/gapi.youtube": "3.0.35",
    "@types/jest": "27.4.1",
    "@types/musickit-js": "1.0.3",
    "@types/node": "17.0.23",
    "@types/node-fetch": "2.6.1",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/react-redux": "7.1.23",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "17.0.1",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.11",
    "@types/styled-components": "5.1.25",
    "@types/testing-library__jest-dom": "5.14.3",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "autoprefixer": "10.4.4",
    "chalk": "5.0.1",
    "commitizen": "4.2.4",
    "cra-template-rb": "1.2.3",
    "cross-env": "7.0.3",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "fontfaceobserver": "2.1.0",
    "http-proxy-middleware": "2.0.4",
    "husky": "7.0.4",
    "inquirer": "8.2.2",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.8",
    "lint-staged": "12.3.7",
    "node-fetch": "2.6.1",
    "node-plop": "0.26.3",
    "plop": "2.7.6",
    "postcss": "8.4.12",
    "postcss-scss": "4.0.3",
    "postcss-styled": "0.34.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.6.2",
    "react-scripts": "5.0.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "serve": "13.0.2",
    "shelljs": "0.8.5",
    "standard-version": "9.3.2",
    "stylelint": "14.6.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "stylelint-scss": "4.2.0",
    "stylelint-selector-bem-pattern": "2.1.1",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.10"
  }
}
